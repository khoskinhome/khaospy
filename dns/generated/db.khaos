;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	piserver.khaos. root.localhost. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	piserver.khaos.
@	IN	A	127.0.0.1
@	IN	AAAA	::1

