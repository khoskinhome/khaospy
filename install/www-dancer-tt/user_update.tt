<!DOCTYPE html>
<html>
    <head>

        [% INCLUDE header_jquery.tt %]

    </head>
    <body>
        <div class='main'>
            <table>
                <tr><td>[% INCLUDE title_linkbar_user.tt %]</td></tr>
            </table>

            <table>

                <form method='post' action='/dancer/user/update'>
                    <tr>
                        <td>Name</td>
                        <td>
                            <input type='text' name='name' value='[% data.name %]' />
                        </td>
                        <td>[% error.name %]</td>
                    </tr>

                    <tr>
                        <td>Email</td>
                        <td><input type='text' name='email' value='[% data.email %]' /></td>
                        <td>[% error.email %]</td>
                    </tr>
                    <tr>
                        <td>Mobile Phone </td>
                        <td>
                            <input type='text' name='mobile_phone' value='[% data.mobile_phone %]' />
                        </td>
                        <td>[% error.mobile_phone %]</td>
                    </tr>
                    <tr>
                        <td></td>
                        <td>
                            <input type='submit' name='update' value='Update'/>
                            <input type='hidden' name='return_url' value='[% return_url %]'/>
                        </td>
                    </tr>
                    <tr><td colspan='2'>[% error_msg %]</td></tr>
                </form>
            </table>
        </div>
    </body>
</html>

